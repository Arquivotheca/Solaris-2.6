# This required package information file contains a list of package contents.
# The 'pkgmk' command uses this file to identify the contents of a package
# and their location on the development machine when building the package.
# Can be created via a text editor or through use of the 'pkgproto' command.

#!search
#!default 0711 root bin
!instdir=./usr/sbin/install.d

# packaging files
i pkginfo=pkginfo
i copyright=copyright
i depend=depend
#
# source locations relative to the prototype file
#
# SUNWinst
#
d none ./usr 0775 root sys
d none ./usr/sbin 0775 root bin
d none $instdir 0775 root bin
f none $instdir/findcd 0755 root bin
f none $instdir/stubboot 0755 root bin
f none $instdir/profind 0755 root sys
f none $instdir/chkprobe 0755 root sys
f none $instdir/pfinstall 0755 root bin
f none $instdir/get_mntpnt 0755 root bin
f none $instdir/set_mntpnt 0755 root bin
f none ./usr/sbin/server_upgrade 0555 root sys
