class="win" name="zenith"
        __wclass__="XSCREEN"
        ddxHandler="ddxSUNWwd.so.1" ddxInitFunc="SUNWwdInit"
        ;

class="win" name="zenith"
        __wclass__="XSCREENCONFIG"
        __category__="display"
	__xinside__="TRUE"
        __title__="prototype ZENITH entry"

        device="zenith"

	desktop="var,__desktop__"
        __desktop__="string,1024x768,1152x900,800x600,640x480"

	res="var,__resolution__"
        __resolution__="string,1024x768,800x600,640x480"

        defdepth="var,__depth__"
        __depth__="string,8,24"

        size="var,__size__"
        __size__="string,7-inch,9-inch,15-inch,17-inch,19-inch"
        __dimx__="string,6,7.5,12,13.5,15"
        __dimy__="string,4,6,9.5,11,12"

        position="var,__position__"
        __position__="string,left,right,top,bottom"
	
        pmifile="/usr/openwin/share/etc/devdata/SUNWaccel/boards/zenith/flex.xqa"
	monitor="mfreq/mfreq48.vda"
        dpix="string,90"
        dpiy="string,90"
        ;
