0x1b    '$'     '*'     ']'|C    -
0x1e    'q'|C   'Q'|C   'q'|N   'Q'|N   CAPS
0x27    'm'|C   'M'|C   'm'|N   'M'|N   CAPS
0x28    0x97    '%'     '''     '%'|N
0x2b    0xe6     0x9c    '`'     -
0x29    0xfd    0xfd    0xfd    0xfd
0x56    '<'     '>'     '\'     -
0x2c    'w'|C   'W'|C   'w'|N   'W'|N   CAPS
