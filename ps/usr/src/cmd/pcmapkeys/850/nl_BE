0x5     '''     '4'     '''|N   '4'|N
0x6     '('     '5'     '('|C   '5'|N
0x7     0xf5     '6'     '^'     '6'|N
0x8     0x8a    '7'     0x8a     '7'|N
0x9     '!'     '8'     '!'|N   '8'|N
0xa     0x87    '9'     '{'     '9'|N
0xb     0x85    '0'     '}'     '0'|N
0xc     ')'     0xf8    ')'|N   0xf8
0xd     '-'     '_'     '-'|N   '_'|N
0x10    'a'|C   'A'|C   'a'|N   'A'|N   CAPS
0x11    'z'|C   'Z'|C   'z'|N   'Z'|N   CAPS
